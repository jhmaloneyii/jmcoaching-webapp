<% provide(:title, "Blog") %>
<div class="container">
  <div class="row">
    <div class="col-lg-12 mx-auto">
      <h1>JM Coaching Blog</h1>
      <table class="table">
        <% @posts.each do |post| %>
          <tr>
            <td><%= render 'shared/post_preview', post: post %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
